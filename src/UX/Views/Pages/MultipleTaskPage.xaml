<Page x:Class="Seemon.Todo.Views.Pages.MultipleTaskPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TextBlock Style="{ThemeResource BodyStrongTextBlockStyle}" x:Uid="MultipleTaskPage_Label" Margin="0 0 0 4"/>
        <TextBox Grid.Row="1" Height="100" TextWrapping="Wrap" AcceptsReturn="True" Text="{x:Bind ViewModel.BindableModel.BindableString, Mode=TwoWay}" />
    </Grid>
</Page>

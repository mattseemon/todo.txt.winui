<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <MenuFlyout x:Key="TBIContextMenu">
        <MenuFlyoutItem x:Uid="TBI_Show" Command="{Binding ShowCommand}" />
        <MenuFlyoutSeparator/>
        <MenuFlyoutItem x:Uid="TBI_Exit" Command="{Binding ExitCommand}" />
    </MenuFlyout>
</ResourceDictionary>
